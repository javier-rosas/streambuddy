{
  "manifest_version": 3,
  "name": "Streambuddy",
  "version": "1.0",
  "description": "A simple hello world extension",
  "action": {
    "default_popup": "index.html"
  },
  "host_permissions": [
    "*://*.netflix.com/*",
    "http://localhost:2000/*",
    "http://167.99.112.197:2000/*",
    "ws://localhost:2000/*",
    "https://unpkg.com/peerjs@1.5.2/dist/peerjs.min.js",
    "https://cdn.socket.io/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.netflix.com/*"
      ],
      "js": [
        "assets/contentScript.js"
      ],
      "type": "module"
    }
  ],
  "background": {
    "service_worker": "assets/background.js",
    "type": "module"
  },
  "oauth2": {
    "client_id": "835250548123-cnovr81e7etgf4hmlm8nn8eoin65nknd.apps.googleusercontent.com",
    "scopes": ["email", "profile"]
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "identity"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "web_accessible_resources": [{
    "matches": ["*://*.netflix.com/*"],
    "resources": ["assets/bundler-GOZcYwuK.js", "assets/_commonjsHelpers-CqkleIqs.js", "assets/index-ChGp7972.js"] 
  }]
}